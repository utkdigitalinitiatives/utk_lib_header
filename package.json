{
  "name": "utk-lib-header",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "ajv": "^6.5.2",
    "core-js": "^2.6.3",
    "dotenv": "^6.0.0",
    "google-map-react": "^1.1.4",
    "lodash": "^4.17.11",
    "moment": "^2.22.1",
    "node-sass-chokidar": "^1.2.2",
    "npm-run-all": "^4.1.2",
    "parse-domain": "^2.1.7",
    "prepend-file": "^1.3.1",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-aspect-ratio": "^1.0.28",
    "react-dom": "^16.4.1",
    "react-keydown": "^1.9.7",
    "react-onclickoutside": "^6.7.1",
    "react-scripts": "1.1.4",
    "replace": "^1.0.0",
    "url-parse": "^1.4.4"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/scss/utk-lib-header.scss -o src/css",
    "watch-css": "npm run build-css && node-sass-chokidar src/scss/utk-lib-header.scss -o src/css --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js build-rename",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "prepend-files": "prepend build/static/js/header.js '/* eslint-disable */'",
    "//": "Build method for polk.lib.utk.edu eXist-db app.",
    "build-www": "npm-run-all build-css build-js build-www-rename build-www-set-absolutes prepend-files ship-www",
    "build-www-rename": "npm-run-all build-www-rename-js build-www-rename-css",
    "build-www-rename-js": "renamer --find '/main[.][^.]+[.]js/' --replace 'header.js' build/static/js/*.js",
    "build-www-rename-css": "renamer --find '/main[.][^.]+[.]css/' --replace 'header.css' build/static/css/*.css",
    "build-www-set-absolutes": "npm-run-all build-www-set-absolutes-js build-www-set-absolutes-css",
    "build-www-set-absolutes-js": "replace 'static[/]' 'wp-content/themes/utk_lib_wp_theme/resources/assets/react/header/' build/static/js/*.js",
    "build-www-set-absolutes-css": "replace '[/]static[/]' '/wp-content/themes/utk_lib_wp_theme/resources/assets/react/header/' build/static/css/*.css",
    "ship-www": "./ship/ship-www.sh",
    "build-polk": "npm-run-all build-css build-js build-polk-rename build-polk-set-absolutes prepend-files ship-polk",
    "build-polk-rename": "npm-run-all build-polk-rename-js build-polk-rename-css",
    "build-polk-rename-js": "renamer --find '/main[.][^.]+[.]js/' --replace 'header.js' build/static/js/*.js",
    "build-polk-rename-css": "renamer --find '/main[.][^.]+[.]css/' --replace 'header.css' build/static/css/*.css",
    "build-polk-set-absolutes": "npm-run-all build-polk-set-absolutes-js build-polk-set-absolutes-css",
    "build-polk-set-absolutes-js": "replace 'static[/]' 'exist/apps/polk-papers/resources/ut-tei/src/vendor/utk_lib_header/' build/static/js/*.js",
    "build-polk-set-absolutes-css": "replace '[/]static[/]' '/resources/ut-tei/src/vendor/utk_lib_header/' build/static/css/*.css",
    "ship-polk": "./ship/ship-polk.sh"
  },
  "homepage": "https://www.lib.utk.edu/",
  "publicPath": "",
  "devDependencies": {
    "renamer": "^1.0.0"
  }
}
